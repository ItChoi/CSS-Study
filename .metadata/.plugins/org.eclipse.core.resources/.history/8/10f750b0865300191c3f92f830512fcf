package com.test.code;

public class Test {

	public static void main(String[] args) {
		Person test = null;
		
		try {
			Person pp = new Person(test);
		} catch (NullNameException e) {
			System.out.println("1");
			e.printStackTrace();
		}
	}
}



class Person {
	
	public Person() { }
	public Person(Person p) throws NullNameException {
		NullNameException ee = new NullNameException("Ёнюс");
		System.out.println("2");
		throw ee;
	}
	
}

class NullNameException extends Exception {
	
	public NullNameException(String message) {
		super(message);
		System.out.println("3");
	}
	
}

